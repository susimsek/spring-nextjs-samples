{
  "name": "spring-nextron-app",
  "version": "1.0.0",
  "private": true,
  "description": "Spring Boot and Nextron full-stack application",
  "license": "UNLICENSED",
  "main": "app/background.js",
  "author": {
    "name": "Şuayb Şimşek",
    "email": "suaybsimsek58@gmail.com",
    "url": "https://github.com/susimsek"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/susimsek/spring-nextjs-samples.git"
  },
  "homepage": "https://github.com/susimsek/spring-nextjs-samples",
  "scripts": {
    "build": "nextron build",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "nextron --renderer-port 8888",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-builder --publish always",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky",
    "prettier:format": "prettier --write */**/*.{md,json,js,ts,html,tsx,css,scss}",
    "build:mac": "nextron build --mac --x64",
    "build:mac:universal": "nextron build --mac --universal",
    "build:win": "nextron build --win --ia32",
    "build:linux": "nextron build --linux"
  },
  "dependencies": {
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9"
  },
  "devDependencies": {
    "@apollo/client": "^3.11.9",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/typescript": "^4.1.1",
    "@graphql-codegen/typescript-operations": "^4.3.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@reduxjs/toolkit": "^2.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-bootstrap": "^0.32.37",
    "@types/react-redux": "^7.1.34",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "husky": "^9.1.6",
    "i18next": "^23.16.4",
    "i18next-resources-for-ts": "^1.5.0",
    "lint-staged": "^15.2.10",
    "next": "^14.2.4",
    "next-i18next": "^15.3.1",
    "nextron": "^9.4.0",
    "picocolors": "^1.1.1",
    "postcss": "^8",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.5",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.1",
    "react-i18next": "^15.1.0",
    "react-redux": "^9.1.2",
    "rimraf": "^3.0.2",
    "sass": "1.77.6",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=21.6.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
